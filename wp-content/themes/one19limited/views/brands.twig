<div class="main-container">
  <main id="primary" class="site-main">
    <header class="page-header">
      <h1 class="page-title">
        {{ wp_title }}
      </h1>
    </header>
    {% for brand in brands %}
    <div class="row">
      <article class="brand">
        <header class="brand-header">
          <div class="brand-logo">
            <a href="{{brand.website}}"><img src="{{ Image(brand.logo).src}}" /></a>
          </div>
          <h3 class="brand-title">{{ brand.title }}</h2>
        </header>
        <div class="brand-main">
          <div class="brand-content">
            {{ brand.content }}
          </div>
        </div>
      </article>
    </div>
    {% else %}
    {{ function('get_template_part', 'template-parts/content', 'none') }};
    {% endfor %}
    <nav class="pagination-container">
      <ul class="pagination-list">
        {% for page in posts.pagination.pages %}
        <li class="pagination-item">
          {% if page.name %}
          <a href="{{ page.link }}" class="{{ page.class }}" {{ page.current ? 'disabled' : '' }}>
            {{ page.title }}
          </a>
          {% else %}
          {{ page.title }}
          {% endif %}
        </li>
        {% endfor %}
      </ul>
    </nav>
  </main>
  <!-- #main -->
</div>